package codingtest;

import java.util.Scanner;

public class ex01 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num;
		
		num = sc.nextInt();
		
		solution(num);
	}


    public static String solution(int num) {
        String answer = "";
        
        if(num%2==0){
            answer ="Even";
        }else{
            answer ="Odd";
        }
        
        return answer;
    }
}